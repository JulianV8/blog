{% extends "layout.twig" %}
{% block content %}
    <a class="btn btn-primary" href="{{ 'admin/users/create' | url }}">New User</a>
    <p></p>
    <table class="table">
        <tr>
            <th>Name</th>
            <th>ID</th>
            <th>Email</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
        {% for user in users %}
            <tr>
                <td>{{ user.nombre }}</td>
                <td>{{ user.cedula }}</td>
                <td>{{ user.email }}</td>
                <td>Edit</td>
                <td>Delete</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}